import java.util.Map;

/**
Alonso
*/
public class PC2{
	private String serialNumber; //serial number for pc2
	private String systemName; //systnem name for pc2
	private Map employeeMap; //map for employee
	
	//consturctor for pc2
	public PC2(String serialNumber, String systemName, Map employeeMap){
		this.serialNumber = serialNumber;
		this.systemName = systemName;
		this.employeeMap = employeeMap;
		
	}
	
	//returns serial number
	public String getSerialNumber(){
		return "";
	}
	
	//returns system name
	public String getSystemName(){
		return "";
	}
	
	public boolean needsReplacement(){
		return false;
	}
	
	public void addEmployee(String serialNumber,Employee2 employee){
		if(!employeeMap.containsKey(serialNumber)){
			employeeMap.put(serialNumber, employee);
			employee.addPC(serialNumber, this);
		}
	}
}
